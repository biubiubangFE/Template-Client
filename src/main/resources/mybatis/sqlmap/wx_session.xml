<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mhdss.dc.dao.WxSessionDAO">

    <sql id="insert_columns">
    (wx_user_id, session_key, c_time, m_time, status)
  </sql>

    <sql id="insert_values">
    (#{wxUserId}, #{sessionKey}, #{cTime}, #{mTime}, #{status})
  </sql>

    <sql id="query_columns">
    id, wx_user_id AS wxUserId, session_key AS sessionKey, c_time AS cTime, m_time AS mTime, status
  </sql>

    <sql id="where">
        <where>

            <if test="wxUserId != null">
                AND wx_user_id = #{wxUserId}
            </if>
            <if test="sessionKey != null">
                AND session_key = #{sessionKey}
            </if>

        </where>
    </sql>

    <sql id="limit_where">
        <include refid="where"/>
        <if test="range != 0">
            LIMIT ${start}, ${range}
        </if>
    </sql>

    <insert id="insert" parameterType="com.mhdss.dc.dataobject.WxSessionDO">
        <selectKey keyProperty="id" resultType="long">
            SELECT LAST_INSERT_ID() AS id
        </selectKey>
        INSERT INTO wx_session
        <include refid="insert_columns"/>
        VALUES
        <include refid="insert_values"/>
    </insert>

    <update id="insertUpdateOnDuplicate" parameterType="com.mhdss.dc.dataobject.WxSessionDO">
        <selectKey keyProperty="id" resultType="long">
            SELECT LAST_INSERT_ID() AS id
        </selectKey>
        INSERT INTO wx_session
        <include refid="insert_columns"/>
        VALUES
        <include refid="insert_values"/>
        ON DUPLICATE KEY UPDATE session_key = #{sessionKey}, id = LAST_INSERT_ID(id)
    </update>

    <update id="updateById" parameterType="com.mhdss.dc.dataobject.WxSessionDO">
        UPDATE wx_session
        <set>
            <if test="wxUserId != null">
                wx_user_id = #{wxUserId},
            </if>
            <if test="sessionKey != null">
                session_key = #{sessionKey},
            </if>
            <if test="cTime != null">
                c_time = #{cTime},
            </if>
            <if test="mTime != null">
                m_time = #{mTime},
            </if>
            <if test="status != null">
                status = #{status},
            </if>
        </set>
        WHERE id = #{id}
    </update>

    <update id="updateByIds" parameterType="java.util.Map">
        UPDATE wx_session
        <set>
            <if test="target.wxUserId != null">
                wx_user_id = #{target.wxUserId},
            </if>
            <if test="target.sessionKey != null">
                session_key = #{target.sessionKey},
            </if>
            <if test="target.cTime != null">
                c_time = #{target.cTime},
            </if>
            <if test="target.mTime != null">
                m_time = #{target.mTime},
            </if>
            <if test="target.status != null">
                status = #{target.status},
            </if>
        </set>
        WHERE id IN
        <foreach close=")" collection="ids" item="id" open="(" separator=",">
            #{id}
        </foreach>
    </update>

    <delete id="deleteById" parameterType="long">
    DELETE FROM wx_session WHERE id = #{id}
  </delete>

    <delete id="deleteByIds" parameterType="java.util.Collection">
        DELETE FROM wx_session WHERE id IN
        <foreach close=")" collection="collection" item="id" open="(" separator=",">
            #{id}
        </foreach>
    </delete>

    <select id="selectById" parameterType="long" resultType="com.mhdss.dc.dataobject.WxSessionDO">
        SELECT
        <include refid="query_columns"/>
        FROM wx_session WHERE id = #{id}
    </select>

    <select id="selectIdsByQuery" parameterType="com.mhdss.dc.query.WxSessionQuery" resultType="long">
        SELECT id FROM wx_session
        <include refid="limit_where"/>
    </select>

    <select id="selectByIds" parameterType="java.util.Collection" resultType="com.mhdss.dc.dataobject.WxSessionDO">
        SELECT
        <include refid="query_columns"/>
        FROM wx_session WHERE id IN
        <foreach close=")" collection="collection" item="id" open="(" separator=",">
            #{id}
        </foreach>
    </select>

    <select id="selectCount" parameterType="com.mhdss.dc.query.WxSessionQuery" resultType="int">
        SELECT COUNT(*) FROM wx_session
        <include refid="where"/>
    </select>

    <select id="selectList" parameterType="com.mhdss.dc.query.WxSessionQuery"
            resultType="com.mhdss.dc.dataobject.WxSessionDO">
        SELECT
        <include refid="query_columns"/>
        FROM wx_session
        <include refid="limit_where"/>
    </select>


    <select id="selectByQuery" parameterType="com.mhdss.dc.query.WxSessionQuery"
            resultType="com.mhdss.dc.dataobject.WxSessionDO">
        SELECT
        <include refid="query_columns"/>
        FROM wx_session
        <include refid="limit_where"/>
    </select>

</mapper>